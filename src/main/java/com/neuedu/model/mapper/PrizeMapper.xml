<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.model.mapper.PrizeMapper" >  

    <select id="getPrizes" parameterType="int" resultType="Prize">
        select *
        from prize
        where aid = #{aid}
    </select>
       
</mapper>