<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.model.mapper.LWMapper" >  

    <select id="getAllLW" resultType="LW">
        select * from lw
    </select>
    
    <select id="getLWById" parameterType="int" resultType="LW">
        select * from lw where lwid = #{lwid}
    </select>
       
</mapper>